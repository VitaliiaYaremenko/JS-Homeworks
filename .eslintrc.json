{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "func-style": ["error", "expression"],
        "quotes": ["error", "single"],
        "func-names": ["error", "never"],
        "camelcase": ["error", { "allow": ["^你好_"] }],
        "no-restricted-syntax": ["error", "ForInStatement", "LabeledStatement", "WithStatement"]
    }
}
